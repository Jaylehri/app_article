<p id='notice'><%= notice %></p>
<h1>Listing Articles</h1>
 <%#= link_to 'New article', new_article_path %>
<%= render  'form',article:@article %>
<table id='article'>
  <thead>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th colspan="3"></th>
  </thead>
  </tr>
  <tbody>
  <% @articles.each do |article| %>
    <%= render partial: 'article', locals: {article: article} %>
    <% end %>
  </tbody>
</table>
<%= will_paginate @articles%>

